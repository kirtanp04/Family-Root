    "version": "0.2.0",
    "configurations": [
    
        {
            "type": "node",
            "request": "launch",
            "name": "Family Tree",
            "skipFiles": ["<node_internals>/**"],
            "program": "${workspaceFolder}/src/Server-Actions/User.ts",
            "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/ts-node",
            "restart": true,
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen",
        },
        {
            "type": "node-terminal",
            "request": "launch",
            "name": "Run Hotel ServerFamily Tree Script",
            "command": "npm run dev",
            "cwd": "${workspaceFolder}/Familt-Tree",
            
        },
    ]
}